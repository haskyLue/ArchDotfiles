URxvt.geometry  : 100x40
 
URxvt.font:xft:Source Code Pro:size=9:style=Regular:antialias=true,xft:Hiragino Sans GB:size=8:style=Regular:antialias=true
URxvt.boldfont:xft:Source Code Pro:size=9:style=Bold:antialias=true,xft:Hiragino Sans GB:size=8:style=Bold:antialias=true
URxvt.letterSpace : 0
URxvt.termName    : rxvt-256color
URxvt.iso14755    : false
 
URxvt.urgentOnBell: true
 
URxvt.saveLines   : 12000
URxvt.scrollBar   : false
URxvt.scrollstyle : rxvt
 
!XFT
Xft.dpi: 96
Xft.lcdfilter : lcddefault
Xft.hintstyle : hintslight
Xft.hinting   : true
Xft.antialias : true
Xft.rgba      : rgb

!Solarized + tangerine + random colours
URxvt.cursorColor            : #93A1A1
URxvt*cursorBlink:      true
 
URxvt.pointerColorBackground : #586E75
URxvt.urlLuncher:     google-chrome-stable
URxvt.pointerColorForeground : #93A1A1
 
URxvt.Depth : 32
URxvt.background : black
URxvt.foreground : white
URxvt*.transparent  : True
URxvt*.shading: 10

!---color---
URxvt.colorBD : #FFFFFF
URxvt.colorUL : #CCCC66
 
! black
URxvt.color0  : #2E3436
URxvt.color8  : #555753
 
! red
URxvt.color1  : #9e1828
URxvt.color9  : #cf6171
 
! green
URxvt.color2  : #aece92
URxvt.color10 : #c5f779
 
! yellow
URxvt.color3  : #968a38
URxvt.color11 : #fff796
 
! blue
URxvt.color4  : #414171
URxvt.color12 : #4186be
 
! magenta
URxvt.color5  : #963c59
URxvt.color13 : #cf9ebe
 
! cyan
URxvt.color6  : #418179
URxvt.color14 : #71bebe
 
! white
URxvt.color7  : #bebebe
URxvt.color15 : #ffffff

!---Extentions---
! URxvt.perl-ext-common : default,keyboard-select,url-select,clipboard,tabbed
! URxvt.tabbed.tabbar-fg: 0
! URxvt.tabbed.tabbar-bg: 0
! URxvt.tabbed.tab-fg: 3
! URxvt.tabbed.tab-bg: 0

URxvt.keysym.M-x           : perl:keyboard-select:activate
URxvt.keysym.M-u           : perl:url-select:select_next
URxvt.keysym.M-c           : perl:clipboard:copy
URxvt.keysym.M-v           : perl:clipboard:paste
URxvt.keysym.M-s           : perl:keyboard-select:search
URxvt.keysym.M-C-v         : perl:clipboard:paste_escaped

URxvt.url-select.launcher  : google-chrome-stable
URxvt.url-select.autocopy  : true
URxvt.url-select.underline : true
